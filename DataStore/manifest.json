{
  "name": "DataStore",
  "version": "1.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "DataStore",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
   "permissions": [ "cookies", "tabs", "http://*/*", "https://*/*", "storage", "downloads", "geolocation"]
}